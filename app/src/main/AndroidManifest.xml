<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.test.newproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PermissionActivity"/>
        <activity android:name=".ClickActivity"/>
        <activity android:name=".activity.AlarmClockActivity"/>
        <activity android:name=".activity.ProgressActivity"/>
        <activity android:name=".activity.CalendarActivity"/>
        <activity android:name=".activity.SelectionModesActivity"/>
        <activity android:name=".activity.CalendarIndexActivity"/>
        <activity android:name=".activity.MultipleSizeActivity"/>
        <activity android:name=".activity.DynamicSettersActivity"/>
        <activity android:name=".activity.CustomTileDimensions"/>
        <activity android:name=".activity.CalendarTestActivity"/>
        <activity android:name=".activity.BasicActivityDecorated"/>
        <activity android:name=".activity.DoubleWaveActivity"/>
        <activity android:name=".activity.RulerActivity"/>
        <activity android:name=".activity.TestActivity"/>
        <activity android:name=".activity.ObjectAnimationActivity"/>
        <activity android:name=".activity.KardiaActivity" android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".activity.PathActivity"/>
    </application>

</manifest>